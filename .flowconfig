[ignore]

[include]

[libs]
flow-typed/*

[lints]

[options]
# Allow require(`some/${template}/path`)
module.ignore_non_literal_requires=true

module.name_mapper='^~\/\(.*\)$' -> '<PROJECT_ROOT>/\1'
module.name_mapper='^~$' -> '<PROJECT_ROOT>'

suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore

[strict]
